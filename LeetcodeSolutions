class Solution {
    public int numJewelsInStones(String J, String S) {
        int jewels = 0;
        for(int i = 0; i < S.length(); i++){
            CharSequence character = String.valueOf(S.charAt(i));
            if(J.contains(character)){
                jewels += 1;    
            }
        }
        return jewels;
    }
}

// will check to see if string S contains any of String J and outputs the number of times this occurs.
